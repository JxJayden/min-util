{
  "name": "min-util",
  "version": "1.2.10",
  "description": "Mini Javascript Util Lib",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test",
    "test-travis": "npm run poor && npm test",
    "poor": "browserify *test*.js | poor -b ie:6..8,chrome -R dot",
    "browser": "browserify . -s _ > browser/browser.js",
    "pregit": "npm run browser && npm run readme && npm run poor",
    "readme": "pretty-readme coveralls gittip > readme.md"
  },
  "author": "chunpu",
  "license": "ISC",
  "dependencies": {
    "min-is": "^1.1.3"
  },
  "devDependencies": {
    "browserify": "latest",
    "istanbul": "^0.3.6",
    "mocha": "^2.1.0",
    "poor": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chunpu/min-util.git"
  },
  "keywords": [
    "util",
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/chunpu/min-util/issues"
  },
  "homepage": "https://github.com/chunpu/min-util"
}
